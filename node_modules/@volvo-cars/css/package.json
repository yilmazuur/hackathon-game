{
  "name": "@volvo-cars/css",
  "description": "Volvo Cars shared CSS",
  "version": "1.33.0",
  "license": "UNLICENSED",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./breakpoints.css": "./dist/breakpoints.css",
    "./debug.css": "./dist/debug.css",
    "./styles.css": "./dist/styles.css",
    "./styles_all-media.css": "./dist/styles_all-media.css",
    "./styles_hover.css": "./dist/styles_hover.css",
    "./styles_md.css": "./dist/styles_md.css",
    "./styles_lg.css": "./dist/styles_lg.css",
    "./tokens.css": "./dist/tokens.css",
    "./font-face.css": "./dist/font-face.css",
    "./meta.json": "./dist/meta.json",
    "./links": {
      "types": "./dist/links.d.ts",
      "node": {
        "import": "./dist/links.server.js",
        "require": "./dist/links.server.cjs"
      },
      "import": "./dist/links.js",
      "require": "./dist/links.cjs"
    },
    "./links.js": {
      "types": "./dist/links.d.ts",
      "node": {
        "import": "./dist/links.server.js",
        "require": "./dist/links.server.cjs"
      },
      "import": "./dist/links.js",
      "require": "./dist/links.cjs"
    },
    "./media-queries": {
      "types": "./dist/media-queries.d.ts",
      "import": "./dist/media-queries.js",
      "require": "./dist/media-queries.cjs"
    },
    "./media-queries.js": {
      "types": "./dist/media-queries.d.ts",
      "import": "./dist/media-queries.js",
      "require": "./dist/media-queries.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./utils.js": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "import": "./dist/tokens.js",
      "require": "./dist/tokens.cjs"
    },
    "./tokens.js": {
      "types": "./dist/tokens.d.ts",
      "import": "./dist/tokens.js",
      "require": "./dist/tokens.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "meta.json": [
        "./dist/meta.json.d.ts"
      ],
      "links": [
        "./dist/links.d.ts"
      ],
      "links.js": [
        "./dist/links.d.ts"
      ],
      "media-queries": [
        "./dist/media-queries.d.ts"
      ],
      "media-queries.js": [
        "./dist/media-queries.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ],
      "utils.js": [
        "./dist/utils.d.ts"
      ],
      "tokens": [
        "./dist/tokens.d.ts"
      ],
      "tokens.js": [
        "./dist/tokens.d.ts"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn run build:css && tsup && yarn run build:css-types",
    "build:dts": "tsc -p tsconfig.build.json",
    "build:prod": "rm -rf dist && yarn run build:css && tsup --sourcemap --minify && yarn run build:css-types",
    "clean": "rm -rf .turbo dist && rm -rf *.tsbuildinfo",
    "dev": "NODE_ENV=development yarn run build:css && NODE_ENV=development yarn run watch:css & yarn run watch:js",
    "test": "yarn run test:jest",
    "test:jest": "yarn run -T jest packages/css",
    "types:check": "tsc -b tsconfig.src-check.json tsconfig.tests.json tsconfig.docs.json --verbose",
    "watch:css": "chokidar \"src/**/*.css\" -c 'yarn run build:css'",
    "watch:js": "tsup --watch",
    "sync:colors": "yarn run css-scripts-sync-colors --core",
    "build:css": "yarn run css-scripts-build-css",
    "build:css-types": "yarn run css-scripts-build-types",
    "spacing": "yarn run css-scripts-spacing --with-additions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/volvo-cars/design-system-web",
    "directory": "packages/css"
  },
  "devDependencies": {
    "@vcc/css-scripts": "workspace:*",
    "@vcc/testing": "workspace:*",
    "@volvo-cars/browserslist-config": "1.4.4",
    "chokidar-cli": "3.0.0",
    "react": "^19.1.0",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
